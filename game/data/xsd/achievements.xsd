<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="list">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="achievement" maxOccurs="unbounded" minOccurs="0">
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="xs:string">
								<xs:attribute type="xs:byte" name="id" use="optional"/>
								<xs:attribute type="xs:string" name="name" use="optional"/>
								<xs:attribute type="xs:string" name="description" use="optional"/>
								<xs:attribute type="xs:string" name="reward" use="optional"/>
								<xs:attribute type="xs:byte" name="minLevel" use="optional"/>
								<xs:attribute type="xs:string" name="mustBeNoble" use="optional"/>
								<xs:attribute type="xs:string" name="mustBeHero" use="optional"/>
								<xs:attribute type="xs:byte" name="minClanLevel" use="optional"/>
								<xs:attribute type="xs:byte" name="minClanMembersCount" use="optional"/>
								<xs:attribute type="xs:string" name="mustBeClanLeader" use="optional"/>
								<xs:attribute type="xs:short" name="minPvPCount" use="optional"/>
								<xs:attribute type="xs:byte" name="minPkCount" use="optional"/>
								<xs:attribute type="xs:byte" name="minWeaponEnchant" use="optional"/>
								<xs:attribute type="xs:string" name="mustBeMarried" use="optional"/>
								<xs:attribute type="xs:string" name="mustBeMageClass" use="optional"/>
								<xs:attribute type="xs:string" name="hasCommonCraft" use="optional"/>
								<xs:attribute type="xs:short" name="crpAmmount" use="optional"/>
								<xs:attribute type="xs:short" name="maxHP" use="optional"/>
								<xs:attribute type="xs:byte" name="minSubclassCount" use="optional"/>
								<xs:attribute type="xs:short" name="mobToHunt" use="optional"/>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>